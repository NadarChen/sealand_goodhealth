page {
    background: #fff;
    padding-bottom: 30rpx;
    font-family: PingFangSC-Regular;
}

.font-Semibold {
    font-family: PingFangSC-Semibold;
}

.chart-line {
    background: #fff;
    padding-top: 20rpx;
    margin-bottom: 50rpx;
}

.chart-title {
    padding-left: 30rpx;
}

.header-region {
    display: flex;
    align-items: center;
}

.header-region .form-item {
    padding: 0;
    margin: 0;
    display: none;
}

.header-region image {
    width: 40rpx;
    height: 40rpx;
}

.content-border {
    background: #fff;
    box-shadow: 0 12rpx 18rpx 0 rgba(10,55,82,0.10);
    border-radius: 15rpx;
    margin: 0 48rpx 32rpx;
}

.content-padding {
    padding: 15rpx 46rpx;
}

.content-header {
    border-bottom: 1rpx solid #ddd;
    margin-bottom: 20rpx;
}

.content-light {
    opacity: 0.7;
    font-family: PingFangSC-Medium;
    font-size: 26rpx;
    color: #999;
    text-align: right;
}

.flex-container {
    display: flex;
    align-items: center;
    position: relative;
}

.justify-item {
    justify-content: space-between;
}

.flex-item {
    flex: 1;
    text-align: center;
}

.back-view {
    position: relative;
    opacity: 0;
    transition: all 0.3s;
}

.back-view.show {
    opacity: 1;
}

.back-image {
    width: 100%;
    height: 534rpx;
    position: fixed;
    top: 0;
    left: 0;
}

.access-box {
    display: flex;
    background: #fff;
    box-shadow: 0 8rpx 20rpx 0 rgba(135,146,164,0.08);
    border-radius: 20rpx;
    margin: 0 32rpx 52rpx 32rpx;
    padding: 38rpx 40rpx;
    align-items: center;
}

.access-box--disabled {
    background: rgba(0,0,0,0.05);
}

.access-box .img {
    width: 100rpx;
    height: 100rpx;
    margin-right: 32rpx;
}

.access-box .img image {
    width: 100rpx;
    height: 100rpx;
}

.access-box .right-content {
    flex: 1;
    text-align: left;
    padding: 0;
}

.right-content .title,.health-code__content .title {
    font-family: PingFangSC-Semibold;
    font-size: 36rpx;
    color: #1E417F;
    font-weight: bold;
    line-height: 50rpx;
}

.right-content .desc,.health-code__content .desc {
    font-size: 24rpx;
    color: #1E417F;
    line-height: 33rpx;
    margin-top: 8rpx;
}

.health-code.access-box {
    position: relative;
    height: 140rpx;
    border-radius: 80rpx;
    padding-left: 0;
    padding-right: 50rpx;
    padding-top: 0;
    padding-bottom: 0;
    justify-content: space-between;
    box-shadow: 0 6px 28px 0 rgba(0,56,118,0.14),inset 0 1px 4px 0 #F1FAFF,inset 0 -1px 3px 0 rgba(198,220,255,0.64);
}

.health-code__imageBg {
    position: absolute;
    width: 100%;
    height: 100%;
}

.health-code__content {
    z-index: 1;
    width: 368rpx;
    margin-left: 7rpx;
    flex: 1;
}

.health-code__image {
    width: 220rpx;
    height: 185rpx;
    z-index: 1;
}

.health-code__image image {
    position: relative;
    bottom: 20rpx;
    width: 100%;
    height: 100%;
}

.health-code__qr-scan {
    font-family: PingFangSC-Medium;
    font-size: 26rpx;
    color: #4293F4;
    text-align: center;
}

.qr-scan-icon image {
    width: 40rpx;
    height: 40rpx;
}

.data-overview-box {
    display: flex;
    flex-direction: column;
    padding: 0;
    padding-top: 32rpx;
    overflow: hidden;
    background: #fff;
    position: relative;
    box-shadow: 0 12rpx 28rpx 0 rgba(135,146,164,0.08);
    border-radius: 20rpx;
    margin: 0 32rpx 60rpx 32rpx;
}

.data-overview-box .data-detail {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 40rpx 0 24rpx 0;
}

.data-overview-box .data-item {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.data-overview-box .header {
    font-family: PingFangSC-Semibold;
    font-size: 36rpx;
    line-height: 50rpx;
    color: #333;
    font-weight: bold;
}

.data-overview-box .header-desc {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    line-height: 37rpx;
    color: #999;
}

.data-overview-box .divider {
    width: 2rpx;
    height: 42rpx;
    background: #ececec;
}

.data-overview-box .data-item .desc {
    color: #5f6068;
    font-size: 26rpx;
    line-height: 37rpx;
    margin-top: 10rpx;
}

.data-overview-box .link-block {
    height: 77rpx;
    background: #f5f7f9;
    padding: 0 32rpx;
    font-size: 26rpx;
    line-height: 37rpx;
}

.data-overview-box .arrow-img {
    width: 24rpx;
    height: 24rpx;
}

.confirm {
    font-family: "DINAlternate-Bold";
    font-size: 52rpx;
    line-height: 61rpx;
    color: #e03333;
}

.suspect {
    font-family: "DINAlternate-Bold";
    font-size: 52rpx;
    color: #e96d0f;
    line-height: 61rpx;
}

.heal {
    font-family: "DINAlternate-Bold";
    font-size: 52rpx;
    color: #37b266;
    line-height: 61rpx;
}

.dead {
    font-family: "DINAlternate-Bold";
    font-size: 52rpx;
    color: #333;
    line-height: 61rpx;
}

.info-block image {
    width: 190rpx;
    height: 160rpx;
    display: block;
    margin: 0 3rpx 0 6rpx;
}

.info-block {
    padding: 0;
}

.wrap-break {
    word-break: break-all;
    word-wrap: break-word;
    white-space: pre-line;
}

.info-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    background: #2872ff;
    border-radius: 27rpx;
    font-size: 26rpx;
    line-height: 37rpx;
    color: #fff;
    width: 112rpx;
    height: 54rpx;
    font-weight: bold;
}

.whole-width {
    width: 100%;
}

.link {
    color: #4293f4;
}

.title-block {
    margin: 56rpx 0 32rpx 32rpx;
    font-size: 44rpx;
    line-height: 62rpx;
    font-weight: bold;
    color: #333;
}

.service-block {
    padding: 0 32rpx;
}

.service-row {
    display: flex;
    justify-content: space-between;
    margin-bottom: 32rpx;
}

.service-col {
    width: 50%;
    box-sizing: border-box;
}

.service-content {
    height: 160rpx;
    padding: 32rpx 15rpx 0 32rpx;
    word-break: keep-all;
    word-wrap: break-word;
    white-space: pre-line;
    text-align: left;
    box-shadow: 0 8rpx 20rpx 0 rgba(135,146,164,0.08);
    border-radius: 20rpx;
}

.service-col .title {
    font-size: 40rpx;
    line-height: 56rpx;
    font-weight: bold;
    color: #333;
    margin-top: 0;
    position: relative;
}

.service-col-bg {
    background-repeat: no-repeat;
    background-size: contain;
    background-position: top right;
}

.kouzhao {
    background-image: url(data:image/png;base64,);
}

.yizhen {
    background-image: url(data:image/png;base64,);
}

.tongxing {
    background-image: url(data:image/png;base64,);
}

