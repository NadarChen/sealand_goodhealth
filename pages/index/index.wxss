    background-image: url(data:image/png;base64,);
}

.wenda {
    background-image: url(data:image/png;base64,);
}

.qyfgba {
    background-image: url(data:image/png;base64,);
}

.qysqxy {
    background-image: url(data:image/png;base64,);
}

.banner-block {
    padding: 49rpx 32rpx 0 32rpx;
}

.banner-block image {
    width: 686rpx;
    height: 160rpx;
}

.service-col image {
    position: absolute;
    right: 0;
    bottom: 0;
    width: 170rpx;
    height: 192rpx;
}

.service-col .title-bg {
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 96rpx;
    height: 20rpx;
    background: rgba(40,114,255,0.16);
    border-radius: 10px;
}

.service-col .desc {
    font-size: 28rpx;
    color: #999;
    margin-top: 8rpx;
}

.fever-block {
    padding: 0 32rpx;
    display: flex;
    justify-content: space-between;
}

.fever-block .img image {
    width: 100rpx;
    height: 100rpx;
    margin-top: 28rpx;
    margin-bottom: 20rpx;
}

.fever-block .fever-item-wrapper {
    width: 33.33%;
    box-sizing: border-box;
}

.fever-block .fever-item {
    height: 224rpx;
    text-align: center;
    font-size: 28rpx;
    box-shadow: 0 8rpx 20rpx 0 rgba(135,146,164,0.08);
    border-radius: 0 0 20rpx 20rpx;
}

.fever-block .item-header {
    height: 8rpx;
    border-radius: 20rpx 20rpx 0 0;
}

.fever-item .desc {
    font-size: 28rpx;
    color: #333;
    margin: 0;
    font-weight: bold;
}

.text--warn {
    color: #db7330;
}

.card-pop {
    display: flex;
    padding: 0 32rpx;
    justify-content: space-between;
}

.card-pop>view {
    height: 192rpx;
    width: 327rpx;
    position: relative;
    border-radius: 20rpx;
    padding: 36rpx 0;
    box-sizing: border-box;
    display: flex;
    align-items: center;
}

.card-left {
    box-shadow: 0 6px 28px 0 rgba(213,70,0,0.30);
}

.card-right {
    box-shadow: 0 6px 28px 0 rgba(0,68,197,0.30);
}

.card-background {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.card-contend {
    display: flex;
    align-items: center;
}

.card-contend>view {
    display: inline-block;
    color: #fff;
}

.card-contend .card-img {
    width: 72rpx;
    height: 72rpx;
    display: inline-block;
    position: relative;
}

.card-title {
    font-weight: bold;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    margin-bottom: 1rpx;
}

.card-title2 {
    font-size: 26rpx;
    margin-bottom: 1rpx;
    opacity: 0.8;
    color: #FFFFFF;
    letter-spacing: 0;
}

.card-text {
    height: 100%;
}

.card-num {
    font-size: 26rpx;
    color: #FFE773;
    letter-spacing: 0;
}

.card-img {
    margin-left: 23rpx;
    margin-right: 20rpx;
}

.u-gm__pop {
    padding: 40rpx;
}

.u-gm__title {
    font-size: 48rpx;
    color: #000000;
    margin-bottom: 30rpx;
}

.u-gm__content {
    font-size: 32rpx;
    color: #000000;
    margin-bottom: 40rpx;
}

.u-gm-btn {
    display: inline-block!important;
    height: 50rpx;
}

.u-gm-left {
    width: 38%;
    vertical-align: top;
}

.u-gm-right {
    width: calc(60% - 20rpx);
    padding-left: 20rpx;
}

.league-block {
    padding: 0 30rpx 0 32rpx;
    display: flex;
    justify-content: space-between;
}

.league-block .league-gird {
    position: relative;
    box-sizing: border-box;
    width: 100%;
}

.league-block .league-gird-item {
    width: calc(25% - 16rpx);
    box-sizing: border-box;
    float: left;
    height: 216rpx;
    text-align: center;
    font-size: 28rpx;
    box-shadow: 0 6rpx 28rpx 0 rgba(135,146,164,0.12);
    border-radius: 12rpx;
    margin-bottom: 16rpx;
    margin-left: 16rpx;
}

.league-block .img image {
    width: 100rpx;
    height: 100rpx;
    margin-top: 30rpx;
    margin-bottom: 16rpx;
}

.league-block .more .more-text {
    text-align: center;
    font-size: 44rpx;
    line-height: 155rpx;
    font-weight: bold;
    color: #333;
}

.league-block .league-gird-item .title {
    font-size: 28rpx;
    color: #333;
    margin: 0;
    font-weight: bold;
    font-family: PingFangSC-Semibold;
    letter-spacing: 0;
    text-align: center;
    line-height: 40rpx;
}

.guoan-img {
    width: 100rpx;
    height: 100rpx;
    display: block;
}

.guobanlist {
    display: flex;
    flex-wrap: wrap;
    padding: 0 16rpx;
}

.guobanlist_item {
    position: relative;
    width: calc(50% - 80rpx);
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 0 32rpx 0;
    box-shadow: 0 6px 28px 0 rgba(135,146,164,0.12);
    border-radius: 16rpx;
    margin-bottom: 32rpx;
    height: 280rpx;
    position: relative;
}

.guobanlist_item_mr {
    margin-right: 32rpx;
}

.guobanlist_item_mb {
    margin-bottom: 32rpx;
}

.guobanlist_item image {
    width: 100rpx;
    height: 100rpx;
    margin-bottom: 20rpx;
}

.guobanlist_item>.title,.guobanlist_item>.desc {
    width: 268rpx;
}

.guobanlist_item>.title {
    letter-spacing: 0;
    text-align: center;
    font-family: PingFangSC-Semibold;
    color: #333333;
    font-size: 36rpx;
    line-height: 50rpx;
    font-weight: bold;
}

.guobanlist_item .desc {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    line-height: 33rpx;
    color: #999999;
    letter-spacing: 0;
    text-align: center;
}

.guoban-item__header {
    height: 8rpx;
    width: calc(100% + 64rpx);
    border-radius: 20rpx 20rpx 0 0;
    margin-bottom: 22rpx;
}

.new {
    position: absolute;
    right: 70rpx;
    top: 20rpx;
}